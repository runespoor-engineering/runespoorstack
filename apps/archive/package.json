{
  "name": "@runespoorstack/archive",
  "description": "A set of documents that may help you to increase overall efficiency for team members and the whole team in general, and to strive to be better engineers.",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "test": "jest --testPathPattern='\\/__tests__\\/(?:unit|integration)/.*\\.(?:test|spec)\\.tsx?$' --ci  2>&1",
    "test:unit": "jest --testPathPattern='\\/__tests__\\/unit/.*\\.(?:test|spec)\\.tsx?$' --ci 2>&1",
    "test:integration": "jest --testPathPattern='\\/__tests__\\/integration\\/.*\\.(?:test|spec)\\.tsx?$' --ci 2>&1",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx,.cjs,.mjs --ignore-path ../../.gitignore --quiet",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx,.cjs,.mjs --ignore-path ../../.gitignore --fix"
  },
  "dependencies": {
    "@docusaurus/core": "3.1.1",
    "@docusaurus/preset-classic": "3.1.1",
    "@mdx-js/react": "3.0.0",
    "clsx": "2.1.0",
    "prism-react-renderer": "2.3.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "@algolia/client-search": "4.22.1",
    "search-insights": "2.13.0",
    "@docusaurus/theme-mermaid": "3.1.1"
  },
  "devDependencies": {
    "@runespoorstack/eslint-config": "workspace:*",
    "@rushstack/eslint-config": "3.6.2",
    "@docusaurus/module-type-aliases": "3.1.1",
    "@docusaurus/types": "3.1.1",
    "prettier": "3.2.5",
    "eslint": "8.56.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-import-resolver-custom-alias": "1.3.2",
    "typescript": "5.3.3",
    "@types/react": "18.2.48"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 3 chrome version",
      "last 3 firefox version",
      "last 5 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/runespoor-engineering/runespoorstack.git"
  },
  "keywords": [
    "runsespoor",
    "engineering",
    "playbook",
    "archive",
    "documentation",
    "learn",
    "articles"
  ],
  "homepage": "https://github.com/runespoor-engineering/runespoorstack/blob/main/apps/archive#readme",
  "bugs": {
    "url" : "https://github.com/runespoor-engineering/runespoorstack/issues"
  },
