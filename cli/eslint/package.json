{
  "name": "@runespoorstack/eslint-cli",
  "version": "0.0.1",
  "description": "The Runespoor CLI for smooth eslint integration and management.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": ["dist"],
  "private": false,
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx,.cjs,.mjs --ignore-path ../../.gitignore --quiet",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx,.cjs,.mjs --ignore-path ../../.gitignore --fix"
  },
  "bin": {
    "rune": "dist/index.js"
  },
  "dependencies": {
    "commander": "13.1.0",
    "@inquirer/input": "4.1.6",
    "@inquirer/select": "4.0.9",
    "@inquirer/confirm": "5.1.6"
  },
  "devDependencies": {
    "@runespoorstack/eslint-config": "workspace:*",
    "@rushstack/eslint-config": "3.6.8",
    "tsup": "8.3.0",
    "typescript": "5.4.5",
    "eslint": "8.57.0",
    "prettier": "3.2.5",
    "vitest":"1.4.0",
    "@types/node": "20.14.8"
  },
  "peerDependencies": {},
  "homepage": "https://github.com/runespoor-engineering/runespoorstack/tree/main/cli/eslint/#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/runespoor-engineering/runespoorstack"
  },
  "bugs": {
    "url": "https://github.com/runespoor-engineering/runespoorstack/issues"
  },
