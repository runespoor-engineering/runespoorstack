{
  "name": "@runespoorstack/changelog-manager",
  "version": "0.7.1",
  "description": "The Runespoor CLI for changelog management and semantic versioning.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": ["dist"],
  "private": false,
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx,.cjs,.mjs --ignore-path ../../.gitignore --quiet",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx,.cjs,.mjs --ignore-path ../../.gitignore --fix"
  },
  "bin": {
    "rune": "dist/index.js"
  },
  "dependencies": {
    "commander": "12.1.0",
    "@inquirer/input": "2.2.3",
    "@inquirer/select": "2.4.1",
    "@inquirer/confirm": "3.1.16",
    "semver": "7.6.3"
  },
  "devDependencies": {
    "@runespoorstack/eslint-config": "workspace:*",
    "@rushstack/eslint-config": "3.6.8",
    "tsup": "8.3.0",
    "typescript": "5.4.5",
    "eslint": "8.57.0",
    "prettier": "3.2.5",
    "vitest":"1.4.0",
    "@types/node": "20.14.8",
    "@types/semver": "7.5.8"
  },
  "peerDependencies": {},
  "homepage": "https://github.com/runespoor-engineering/runespoorstack/tree/main/cli/changelog/#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/runespoor-engineering/runespoorstack"
  },
  "bugs": {
    "url": "https://github.com/runespoor-engineering/runespoorstack/issues"
  },
  "author": {
    "name": "BorysShulyak",
    "email": "borysshulyak@gmail.com",
    "url": "https://github.com/BorysShulyak"
  },
  "maintainers": [
    {
      "name": "BorysShulyak",
      "email": "borysshulyak@gmail.com",
      "url": "https://github.com/BorysShulyak"
    }
  ],
  "keywords": [
    "changelog",
    "generator",
    "semver",
    "versioning",
    "runespoor"
  ],
  "license": "MIT"
}
